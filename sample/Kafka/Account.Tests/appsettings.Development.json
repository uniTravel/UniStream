{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kafka": {
    "Aggregate": {
      "Producer": {
        "BootstrapServers": "localhost:9192,localhost:9292,localhost:9392"
      },
      "Consumer": {
        "BootstrapServers": "localhost:9192,localhost:9292,localhost:9392",
        "GroupId": "group",
        "EnablePartitionEof": true
      }
    }
  },
  "Aggregate": {
    "Account": {
      "Capacity": 10000,
      "Refresh": 0.2
    },
    "Transaction": {
      "Capacity": 10000,
      "Refresh": 0.2
    }
  }
}